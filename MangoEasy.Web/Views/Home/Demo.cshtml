@{
    ViewBag.Title = "Demo";
    Layout = "~/Views/Shared/_EmptyLayout.cshtml";
    
}
@Scripts.Render("~/bundles/demo")
<a style="padding-top: 10px" href="#" data-bind="click:Demo.viewModel.Record,text:Demo.viewModel.RecordState()>0?'停止录音':'开始录音',css:Demo.viewModel.profitStatus"></a>

<p data-bind="text:Demo.viewModel.Tips" class="text-center" style="padding-top: 10px"></p>
<table class="table">
    <thead>
        <tr>
            <th>时间</th>
            <th>文件</th>
            @*<th>操作</th>*@
        </tr>
    </thead>
    <tbody data-bind="foreach:Demo.viewModel.Orders">
        <tr>
            <td data-bind="text:CreatedTime"></td>
            <td data-bind="text:FileId"></td>
            @*<td>
                <a data-bind="attr:{href:Url}" target="_blank">下载</a>
            </td>*@
        </tr>
    </tbody>
</table>





